    <PackageReference Include="DevExpress.Printing.Core" Version="22.2.5" />
    <PackageReference Include="DevExpress.Wpf.Core" Version="22.2.5" />
    <PackageReference Include="DevExpress.Wpf.PivotGrid" Version="22.2.5" />
    <PackageReference Include="DevExpress.Wpf.Printing" Version="22.2.5" />
    <PackageReference Include="DevExpress.Wpf.Themes.All" Version="22.2.5" />
    <PackageReference Include="System.Data.OleDB" Version="5.0" />
  </ItemGroup>
</Project>
